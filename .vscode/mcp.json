{
	"servers": {
		"supabase": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@supabase/mcp-server-supabase@0.4.5"
			],
			"env": {
				"SUPABASE_URL": "https://myproject.supabase.co",
				"SUPABASE_SERVICE_KEY": "super-secret-service-key"
			}
		}
	},
	"inputs": [
		{
			"type": "promptString",
			"id": "supabase-access-token",
			"description": "Supabase personal access token",
			"password": true
		}
	]
}