{
	"servers": {
		"supabase": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@supabase/mcp-server-supabase@latest",
				"--project-ref",
				"${env:SUPABASE_PROJECT_REF}",
				"--read-only"
			],
			"envFile": "${workspaceFolder}/.env.local",
			"env": {
				"SUPABASE_ACCESS_TOKEN": "${env:SUPABASE_ACCESS_TOKEN}"
			},
			"cwd": "${workspaceFolder}"
		},
		"playwright": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"type": "stdio"
		}
	}
}